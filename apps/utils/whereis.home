// home-os Whereis
// Locate binary, source, and manual page

import "../../kernel/src/core/foundation.home" as foundation

export fn whereis_main(command: u64) {
  foundation.vga_write_string(command)
  foundation.vga_write_string(": /usr/bin/")
  foundation.vga_write_string(command)
  foundation.vga_write_string(" /usr/share/man/man1/")
  foundation.vga_write_string(command)
  foundation.vga_write_string(".1\n")
}
