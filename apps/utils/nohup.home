// home-os Nohup
// Run command immune to hangups

import "../../kernel/src/core/foundation.home" as foundation

export fn nohup_main(command: u64) {
  foundation.vga_write_string("nohup: ignoring input and appending output to 'nohup.out'\n")
  
  // Execute command in background (stub)
  foundation.vga_write_string("Running: ")
  foundation.vga_write_string(command)
  foundation.vga_write_string("\n")
}
